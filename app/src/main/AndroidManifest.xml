<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.noteapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NoteApplication">
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/Theme.NoteApplication.NoActionBar">

        </activity>
        <activity android:name=".Note_Activity" />
        <activity android:name=".Status_Activity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Priority_Activity" />
        <activity android:name=".Dashboard_Activity" />
        <activity android:name=".LogIn_Activity" />
        <activity android:name=".Category_Activity">

        </activity>
    </application>

</manifest>